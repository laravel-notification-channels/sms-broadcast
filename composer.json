{
    "name": "laravel-notification-channels/sms-broadcast",
    "description": "SMS Broadcast Notification Channel",
    "homepage": "http://laravel-notification-channels.com/",
    "license": "MIT",
    "require": {
        "php": ">=7.2.5",
        "atymic/sms-broadcast": "^1.0",
        "illuminate/notifications": "~5.6 || ~6.0 || ~7.0",
        "illuminate/support": "~5.6 || ~6.0 || ~7.0"
    },
    "authors": [
        {
            "name": "atymic",
            "email": "atymicq@gmail.com",
            "homepage": "https://atymic.dev",
            "role": "Developer"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\SmsBroadcast\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\SmsBroadcast\\SmsBroadcastServiceProvider"
            ]
        }
    }
}
